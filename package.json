{
  "name": "adl-investment",
  "description": "Investment management application for ADL",
  "version": "1.0.0",
  "scripts": {
    "vite-build": "tsc -b && vite build",
    "build:css": "tailwindcss -i ./resources/css/style.css -o ./public/css/style.css --watch",
    "lint": "eslint .",
    "start": "node www/src/index.js",
    "dev": "clear && nodemon --exec ts-node src/index.ts",
    "build": "clear && rm -rf www/* && tsc && cp -r views www && cp package.json www && cp -r public www && cp -r src www && cp -r prisma www",
    "prisma-pull": "npx prisma db pull",
    "prisma-generate": "npx prisma generate",
    "test": "mocha --require test/support/env --reporter spec --bail --check-leaks test/ test/acceptance/",
    "test-ci": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=lcovonly --reporter=text npm test",
    "test-cov": "nyc --exclude examples --exclude test --exclude benchmarks --reporter=html --reporter=text npm test",
    "test-tap": "mocha --require test/support/env --reporter tap --check-leaks test/ test/acceptance/"
  },
  "license": "MIT",
  "repository": "expressjs/express",
  "homepage": "http://expressjs.com/",
  "keywords": [
    "investment",
    "finance",
    "portfolio",
    "stocks",
    "bonds",
    "mutual funds",
    "online gaming",
    "esports",
    "gambling",
    "betting",
    "casino"
  ],
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "accepts": "^2.0.0",
    "body-parser": "^2.0.1",
    "content-disposition": "^1.0.0",
    "content-type": "~1.0.4",
    "cookie": "0.6.0",
    "cookie-signature": "^1.2.1",
    "debug": "4.3.6",
    "depd": "2.0.0",
    "dotenv": "^16.3.2",
    "encodeurl": "~2.0.0",
    "escape-html": "~1.0.3",
    "etag": "~1.8.1",
    "express": "^4.21.0",
    "finalhandler": "^2.0.0",
    "fresh": "2.0.0",
    "fs": "^0.0.1-security",
    "http-errors": "2.0.0",
    "in_array": "^1.1.0",
    "merge-descriptors": "^2.0.0",
    "methods": "~1.1.2",
    "mime-types": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "on-finished": "2.4.1",
    "once": "1.4.0",
    "parseurl": "~1.3.3",
    "path": "^0.12.7",
    "proxy-addr": "~2.0.7",
    "qs": "6.13.0",
    "range-parser": "~1.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "router": "^2.0.0",
    "safe-buffer": "5.2.1",
    "send": "^1.1.0",
    "serve-static": "^2.1.0",
    "setprototypeof": "1.2.0",
    "socket.io": "^4.7.5",
    "statuses": "2.0.1",
    "type-is": "^2.0.0",
    "utils-merge": "1.0.1",
    "uuid": "^8.3.2",
    "vary": "~1.1.2"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/http-errors": "^1.8.2",
    "@types/morgan": "^1.9.3",
    "@types/multer": "^1.4.12",
    "@types/node": "^18.6.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.2",
    "after": "0.8.2",
    "autoprefixer": "^10.4.20",
    "connect-redis": "3.4.2",
    "cookie-parser": "1.4.6",
    "cookie-session": "2.0.0",
    "daisyui": "^4.12.13",
    "ejs": "3.1.9",
    "eslint": "8.47.0",
    "express-session": "1.17.2",
    "hbs": "4.2.0",
    "marked": "0.7.0",
    "method-override": "3.0.0",
    "mocha": "10.2.0",
    "morgan": "1.10.0",
    "nodemon": "^3.1.4",
    "nyc": "15.1.0",
    "pbkdf2-password": "1.2.1",
    "postcss": "^8.4.47",
    "prisma": "^5.20.0",
    "sass-embedded": "^1.79.4",
    "supertest": "6.3.0",
    "tailwindcss": "^3.4.13",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
    "vhost": "~3.0.2",
    "vite": "^5.4.8"
  },
  "engines": {
    "node": ">= 18"
  },
  "files": [
    "LICENSE",
    "History.md",
    "Readme.md",
    "index.js",
    "lib/"
  ]
}
